{
  "manifest_version": 3,
  "name": "Ad Blocker",
  "version": "1.0",
  "description": "Blocks unwanted ads and popups for a cleaner browsing experience.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "clipboardWrite",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ]
}